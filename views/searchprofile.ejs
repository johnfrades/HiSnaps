<% include partials/header %>




<div class="container">	

<div class="row">
<% foundUser.forEach(function(theUser){ %>

<div class="col-lg-4">
	<div class="ui card">
  <div class="image">
    <img src="<%= theUser.image %>">
  </div>
  <div class="content">
    <a class="header" href="/profile/<%= theUser._id%>"><%= theUser.firstname %> <%= theUser.lastname %> </a>
    <div class="meta">
      <span class="date">Joined in 2013</span>
    </div>
    <div class="description">
      Kristy is an art director living in New York.
    </div>
  </div>
  <div class="extra content">
    <a>
      <i class="user icon"></i>
      22 Friends
    </a>
  </div>
</div>
</div>
<% }); %>


</div>
</div>





<% include partials/footer %>